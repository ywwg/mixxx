<!DOCTYPE template>
<!--
  Description:
    Effect selector buttons.
  Variables:
    EffectRack: the rack
    EffectUnit: the chain
    Effect: the effect
-->
<Template>
	<WidgetGroup>
		<Layout>horizontal</Layout>
		<Style> WEffect, WPushButton { 
			font: bold 12px/10px sans-serif;
			font-family: "Open Sans";
			background-color: transparent; 
			color: #191F24; 
			padding-left: 1px;
		}
		</Style>
		<Pos>1,1</Pos>
		<Children>
            <EffectSelector>
		        <EffectRack>1</EffectRack>
		        <EffectUnit><Variable name="effectunitnum"/></EffectUnit>
		        <Effect><Variable name="effectnum"/></Effect>
				<Size>105f,20f</Size>	
		     </EffectSelector>
		</Children>
	</WidgetGroup>
</Template>
